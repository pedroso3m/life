@model IList<DomainCompany.Entities.Banner>
@if (Model != null && Model.Any())
{
<div class="ui container">
    <div class="ui four images middle aligned center aligned">
        
            @foreach (var banner in Model)
            {
                <a href="@banner.Link" class="ui image" title="@banner.Name">
                    <img src="/assets/image/img_default.png" data-src="@banner.NameFileImage" alt="@banner.Name" title="@banner.Name" class="margin none" class="ui image">
                </a>
            }
        
    </div>
</div>
}